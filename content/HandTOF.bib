@article{lindner_time--flight_2010,
	title = {Time-of-{Flight} sensor calibration for accurate range sensing},
	volume = {114},
	issn = {1077-3142},
	url = {http://www.sciencedirect.com/science/article/pii/S1077314210001682},
	doi = {10.1016/j.cviu.2009.11.002},
	abstract = {Over the past years Time-of-Flight (ToF) sensors have become a considerable alternative to conventional distance sensing techniques like laser scanners or image based stereo-vision. Due to the ability to provide full-range distance information at high frame-rates, ToF sensors achieve a significant impact onto current research areas like online object recognition, collision prevention or scene and object reconstruction. Nevertheless, ToF-cameras like the Photonic Mixer Device (PMD) still exhibit a number of error sources that affect the accuracy of measured distance information. For this reason, major error sources for ToF-cameras will be discussed, along with a new calibration approach that combines intrinsic, distance as well as a reflectivity related error calibration in an overall, easy to use system and thus significantly reduces the number of necessary reference images. The main contribution, in this context, is a new intensity-based calibration model that requires less input data compared to other models and thus significantly contributes to the reduction of calibration data.},
	number = {12},
	urldate = {2015-10-05},
	journal = {Computer Vision and Image Understanding},
	author = {Lindner, Marvin and Schiller, Ingo and Kolb, Andreas and Koch, Reinhard},
	month = dec,
	year = {2010},
	keywords = {Calibration, Photonic Mixer Device, Time-of-Flight},
	pages = {1318--1328},
	file = {Lindner2010TimeofFlightSensorCalibration.pdf:C\:\\Users\\t-srisri\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\xwbbhxnu.default\\zotero\\storage\\4NPTJAWK\\Lindner2010TimeofFlightSensorCalibration.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\t-srisri\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\xwbbhxnu.default\\zotero\\storage\\CJUSHUU3\\S1077314210001682.html:}
}

@article{velten_recovering_2012,
	title = {Recovering three-dimensional shape around a corner using ultrafast time-of-flight imaging},
	volume = {3},
	copyright = {© 2012 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	url = {http://www.nature.com/ncomms/journal/v3/n3/abs/ncomms1747.html},
	doi = {10.1038/ncomms1747},
	abstract = {The recovery of objects obscured by scattering is an important goal in imaging and has been approached by exploiting, for example, coherence properties, ballistic photons or penetrating wavelengths. Common methods use scattered light transmitted through an occluding material, although these fail if the occluder is opaque. Light is scattered not only by transmission through objects, but also by multiple reflection from diffuse surfaces in a scene. This reflected light contains information about the scene that becomes mixed by the diffuse reflections before reaching the image sensor. This mixing is difficult to decode using traditional cameras. Here we report the combination of a time-of-flight technique and computational reconstruction algorithms to untangle image information mixed by diffuse reflection. We demonstrate a three-dimensional range camera able to look around a corner using diffusely reflected light that achieves sub-millimetre depth precision and centimetre lateral precision over 40 cm×40 cm×40 cm of hidden space.
View full text},
	language = {en},
	urldate = {2015-10-06},
	journal = {Nature Communications},
	author = {Velten, Andreas and Willwacher, Thomas and Gupta, Otkrist and Veeraraghavan, Ashok and Bawendi, Moungi G. and Raskar, Ramesh},
	month = mar,
	year = {2012},
	keywords = {Applied physics, Optical physics, Physical sciences},
	pages = {745},
	file = {corner-prep.pdf:C\:\\Users\\t-srisri\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\xwbbhxnu.default\\zotero\\storage\\H2H9ZD56\\corner-prep.pdf:application/pdf;Snapshot:C\:\\Users\\t-srisri\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\xwbbhxnu.default\\zotero\\storage\\K8FT4TWA\\ncomms1747.html:}
}

@article{heide_low-budget_2013,
	title = {Low-budget {Transient} {Imaging} {Using} {Photonic} {Mixer} {Devices}},
	volume = {32},
	issn = {0730-0301},
	url = {http://doi.acm.org/10.1145/2461912.2461945},
	doi = {10.1145/2461912.2461945},
	abstract = {Transient imaging is an exciting a new imaging modality that can be used to understand light propagation in complex environments, and to capture and analyze scene properties such as the shape of hidden objects or the reflectance properties of surfaces. Unfortunately, research in transient imaging has so far been hindered by the high cost of the required instrumentation, as well as the fragility and difficulty to operate and calibrate devices such as femtosecond lasers and streak cameras. In this paper, we explore the use of photonic mixer devices (PMD), commonly used in inexpensive time-of-flight cameras, as alternative instrumentation for transient imaging. We obtain a sequence of differently modulated images with a PMD sensor, impose a model for local light/object interaction, and use an optimization procedure to infer transient images given the measurements and model. The resulting method produces transient images at a cost several orders of magnitude below existing methods, while simultaneously simplifying and speeding up the capture process.},
	number = {4},
	urldate = {2015-10-08},
	journal = {ACM Trans. Graph.},
	author = {Heide, Felix and Hullin, Matthias B. and Gregson, James and Heidrich, Wolfgang},
	month = jul,
	year = {2013},
	keywords = {computational photography, transient imaging},
	pages = {45:1--45:10},
	file = {HeideEtAl-LowBudgetTransientImaging-SIGGRAPH2013.pdf:C\:\\Users\\t-srisri\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\xwbbhxnu.default\\zotero\\storage\\HPPX3IRN\\HeideEtAl-LowBudgetTransientImaging-SIGGRAPH2013.pdf:application/pdf}
}

